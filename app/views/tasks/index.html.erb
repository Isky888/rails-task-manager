<h1>Tasks</H1>
<%@tasks.each do |task|%>
<ul>
 <li><%=task.title%></li>
 <li><%=task.details%></li>
 <li>Completed: <%=task.completed%></li>
 <li><%=link_to "View Task", task_path(task)%></li>
</ul>
<hr>
<%end%>
<p>Would you like to add a new task?</p>
<%=link_to 'Create new', new_task_path(@task)%>
